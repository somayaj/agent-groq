{
  "name": "agent-groq",
  "version": "1.0.4",
  "type": "module",
  "description": "LangChain-powered AI agent using Groq for fast, free inference. Features custom tool creation, AI code generation, and beautiful web interface.",
  "main": "src/agent.js",
  "exports": {
    ".": {
      "import": "./src/agent.js",
      "default": "./src/agent.js"
    },
    "./tools": {
      "import": "./src/tools/index.js",
      "default": "./src/tools/index.js"
    },
    "./server": {
      "import": "./src/server.js",
      "default": "./src/server.js"
    }
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "server": "node src/server.js",
    "server:dev": "node --watch src/server.js"
  },
  "keywords": [
    "langchain",
    "groq",
    "agent",
    "ai",
    "llm",
    "chatbot",
    "tools",
    "llama",
    "react-agent",
    "artificial-intelligence"
  ],
  "author": "Asha Somayajula",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/somayaj/agent-groq.git"
  },
  "bugs": {
    "url": "https://github.com/somayaj/agent-groq/issues"
  },
  "homepage": "https://github.com/somayaj/agent-groq#readme",
  "files": [
    "src/",
    "public/",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/groq": "^0.1.0",
    "@langchain/langgraph": "^0.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "zod": "^3.23.0"
  }
}
